{
  "name": "segura",
  "version": "1.0.0",
  "description": "A CLI tool which helps in pushing uncommitted code in case of emergencies or distress",
  "main": "build/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/karthickram286/segura"
  },
  "bin": {
    "segura": "bin/segura"
  },
