  "author": "Karthick Ram",
  "license": "MIT",
  "dependencies": {
    "esm": "^3.2.25",
    "listr": "^0.14.3",
    "simple-git": "^3.15.0"
  },
  "devDependencies": {
    "@types/listr": "^0.14.2",
    "@types/node": "^14.0.27",
    "typescript": "^3.9.7"
  }
